{
  "scopeName": "source.vue",
  "patterns": [
    {
      "include": "#template"
    }
  ],
  "repository": {
    "template": {
      "patterns": [
        {
          "include": "#cannonui"
        }
      ]
    },
    "cannonui": {
      "match": "<\\w+-(\\w+)",
      "name": "entity.name.tag.custom.vue"
    }
  }
}
